                                    <div class="row col-12 justify-content-end">
                                        <div class="col-6 col-md-3 mt-md-0 mt-1 d-grid">
                                            <button type="button" class="btn btn-label-success waves-effect" data-bs-toggle="modal" data-bs-target="#einvoice-serie-modal">
                                                <i class="fa-solid fa-plus me-1"></i> Yeni e-Fatura Serisi
                                            </button>
                                            <div class="modal modal-top fade" id="einvoice-serie-modal" tabindex="-1" style="display: none;" aria-hidden="true">
                                                <div class="modal-dialog">
                                                    <form class="modal-content">
                                                        <div class="modal-header">
                                                            <h5 class="modal-title" id="modalTopTitle">Yeni e-Fatura Serisi</h5>
                                                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                        </div>
                                                        <div class="modal-body">
                                                            <div class="row">
                                                                <div class="col mb-3">
                                                                    <label for="einvoice-serie-name" class="form-label">Seri</label>
                                                                    <input type="text" id="einvoice-serie-name" class="form-control" placeholder="Seri Kodu">
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col">
                                                                    <label class="switch switch-square">
                                                                        <input id="einvoice-isdefault" type="checkbox" class="switch-input" />
                                                                        <span class="switch-toggle-slider">
                                                                            <span class="switch-on"><i class="ti ti-check"></i></span>
                                                                            <span class="switch-off"><i class="ti ti-x"></i></span>
                                                                        </span>
                                                                        <span class="switch-label">Varsayılan</span>
                                                                    </label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="modal-footer">
                                                            <button id="einvoice-save-serie" type="button" class="btn btn-primary waves-effect waves-light">Kaydet</button>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <table id="einvoice-series-table" class="table table-hover card-table">
                                        <thead>
                                            <tr>
                                                <th>id</th>
                                                <th>Seri Kodu</th>
                                                <th>Varsayılan</th>
                                                <th>Durum</th>
                                                <th>İşlem</th>
                                            </tr>
                                        </thead>
                                    </table>
